CREATE TABLE PUBLIC.BANKACCOUNT(BRANCH_CODE VARCHAR(10), ACCOUNT_NUMBER VARCHAR(10), CUST_NAME VARCHAR(20),
								 CUST_ADDRESS VARCHAR(50), CUST_RATING DECIMAL(3), BALANCE DECIMAL(7));
								 
